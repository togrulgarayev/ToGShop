    <header>

        <nav id="togNav" class="navbar navbar-expand-lg navbar-light">

            <div class="container">
                <div class="navbar-brand">
                    <a asp-controller="Home" asp-action="Index"><img draggable="false" width="100" src="~/assets/img/nav-logo.png"></a>
                    <div class="logo-line"></div>
                    <div id="logo-category" onclick="SidenavOpenClose()" class="logo-category"> <i id="icon" class="icon bi bi-folder2"></i> <p class="logo-category-text">Kateqoriyalar</p></div>
                    <!-- <i class="fas fa-folder-open"></i> -->
                </div>




                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="bi bi-list"></i>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">

                    <div class="search-responsive">
                        <form class="search" id="form">
                            <div class="search-input">
                                <input type="search" id="query" name="q" placeholder="Axtar...">
                            </div>
                            <button class="search-btn">Axtar</button>
                        </form>
                    </div>

                    <ul class="navbar-nav ml-auto">

                        <li class="nav-item text-center">
                            <a class="nav-link" asp-controller="Cart" asp-action="Index"><i class="fas fa-shopping-cart"></i> 2</a>
                        </li>

                        <li class="nav-item text-center dropdown">
                            <a class="nav-link dropdown-toggle" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fas fa-language"></i> Dil
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <a style="cursor: pointer" class="dropdown-item text-center"><img height="20" draggable="false" src="~/assets/flags/az.svg" alt=""> <p style="display: inline-block ;margin-left: 2px">Aze</p></a>
                                <a style="cursor: pointer" class="dropdown-item text-center"><img height="20" draggable="false" src="~/assets/flags/gb.svg" alt=""> <p style="display: inline-block ;margin-left: 2px">Eng</p></a>
                            </div>
                        </li>

                        <li class="nav-item text-center dropdown">
                            <a class="nav-link dropdown-toggle" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fas fa-user"></i> Hesab
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <a style="cursor: pointer" asp-controller="Account" asp-action="Index" class="dropdown-item my-login-btn text-center"><i class="bi bi-box-arrow-in-right"></i> <p style="display: inline-block ;margin-left: 2px">Daxil ol</p></a>
                            </div>
                        </li>


                    </ul>


                </div>
            </div>
        </nav>


        <section class="side-navbar">
            <div id="mySidenav" class="sidenav">
                <a class="dropdown-btn category-title"><i class="bi bi-laptop"></i> <p>Kompüterlər</p></a>
                <div class="dropdown-container">
                    <a class="text-center category-item" href="#"><p>Apple</p></a>
                    <a class="text-center category-item" href="#"><p>Lenovo</p></a>
